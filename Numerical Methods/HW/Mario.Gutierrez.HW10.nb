      RowBox[{
       StyleBox["+",
        Background->None], 
       RowBox[{
        StyleBox[
         RowBox[{
          RowBox[{"u", "'"}], 
          RowBox[{"(", 
           SubscriptBox["x", "i"], ")"}], "\[CapitalDelta]", " ", "x"}],
         Background->None], 
        RowBox[{
         StyleBox["+",
          Background->None], 
         RowBox[{
          StyleBox[
           FractionBox["1", "2"],
           Background->None], 
          RowBox[{"u", "''"}], 
          RowBox[{"(", 
           SubscriptBox["x", "i"], ")"}], 
          SuperscriptBox[
           RowBox[{"(", 
            RowBox[{"\[CapitalDelta]", " ", "x"}], ")"}], "2"]}]}]}]}]}]}]}], 
   TraditionalForm]]],
 "\n      \[DoubleLongRightArrow] ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    StyleBox[
     RowBox[{"u", "(", 
