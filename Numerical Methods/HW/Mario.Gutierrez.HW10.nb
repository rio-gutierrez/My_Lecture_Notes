       RowBox[{"(", 
        RowBox[{
         SubscriptBox["x", 
          RowBox[{"i", "+", "1"}]], "-", 
         SubscriptBox["x", "i"]}], ")"}]}], "+", 
      RowBox[{
       FractionBox["1", "2"], 
       RowBox[{"f", "'"}], 
