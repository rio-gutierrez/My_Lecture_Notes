          SubscriptBox["x", "i"], ")"}]}], ")"}], 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{
          SubscriptBox["x", 
           RowBox[{"i", "+", "1"}]], "-", 
          SubscriptBox["x", "i"]}], ")"}], "2"]}]}]}],
    Background->None], TraditionalForm]]],
 "\nor \n            ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    StyleBox[" ",
     Background->None], 
    RowBox[{
     StyleBox[
      RowBox[{"u", "(", 
       SubscriptBox["x", 
        RowBox[{"i", "+", "1"}]], ")"}],
      Background->None], 
     StyleBox["=",
      Background->None], 
     RowBox[{
      StyleBox[
       RowBox[{"u", "(", 
