Cell[TextData[{
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " denotes the conjunctive ",
 StyleBox["and ",
  FontSlant->"Italic"],
 "by ",
 StyleBox["&&",
  FontWeight->"Bold"],
 ". The conjunctive ",
 StyleBox["or",
  FontSlant->"Italic"],
 " is denoted by ",
 StyleBox["||. ",
  FontWeight->"Bold"],
 "The negation ",
 StyleBox["not",
  FontSlant->"Italic"],
 " is denoted by ",
 StyleBox["!",
  FontWeight->"Bold"],
 ","
}], "Text",
 CellChangeTimes->{{3.557866694004861*^9, 3.557866695176209*^9}, {
  3.557866808801161*^9, 3.557866987409176*^9}}],

