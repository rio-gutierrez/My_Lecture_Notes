 Background->RGBColor[0.87, 0.94, 1]],

Cell[TextData[{
 StyleBox["a:",
  FontWeight->"Bold"],
 " Use ",
 StyleBox["RandomRational",
  FontWeight->"Bold"],
 " to test whether ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    RowBox[{"(", 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"-", "1"}], ")"}], 
      RowBox[{"1", "/", 
       RowBox[{"Denominator", "[", "q", "]"}]}]], ")"}], 
    RowBox[{"Numerator", "[", "q", "]"}]], TraditionalForm]]],
 " is identical with ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    RowBox[{"(", 
     RowBox[{"-", "1"}], ")"}], "q"], TraditionalForm]]],
 ". Use 50000 test cases between 1 and 10000."
}], "Text",
 CellChangeTimes->{{3.554222554265896*^9, 3.5542227169588013`*^9}, {
